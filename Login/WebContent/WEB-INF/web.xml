<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>Login</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>Servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>IUserDAO</servlet-name>
    <servlet-class>Dao.IUserDAO</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IUserDAO</servlet-name>
    <url-pattern>/IUserDAO</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserDAOImpI</servlet-name>
    <servlet-class>Dao.UserDAOImpI</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserDAOImpI</servlet-name>
    <url-pattern>/UserDAOImpI</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserDAOProxy</servlet-name>
    <servlet-class>Dao.proxy.UserDAOProxy</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserDAOProxy</servlet-name>
    <url-pattern>/UserDAOProxy</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DatabaseConnection</servlet-name>
    <servlet-class>Dbc.DatabaseConnection</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DatabaseConnection</servlet-name>
    <url-pattern>/DatabaseConnection</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>User</servlet-name>
    <servlet-class>Vo.User</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>User</servlet-name>
    <url-pattern>/User</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>Login.html</welcome-file>
    <welcome-file>Login.htm</welcome-file>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
</web-app>